{
  "name": "@chrryai/waffles",
  "version": "1.2.60",
  "description": "Production-ready Playwright testing utilities and helpers for modern web apps",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src"
  },
  "keywords": [
    "playwright",
    "testing",
    "e2e",
    "test-utils",
    "test-helpers",
    "automation",
    "typescript"
  ],
  "author": "Ibrahim Velinov",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/chrryai/waffles.git"
  },
  "bugs": {
    "url": "https://github.com/chrryai/waffles/issues"
  },
  "homepage": "https://github.com/chrryai/waffles",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@playwright/test": "^1.40.0",
    "react": "^18.0.0 || ^19.0.0",
    "@chrryai/chrry": "workspace:*"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.1",
    "@types/node": "^22.15.3",
    "@types/react": "^18.0.0",
    "eslint": "^9.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@faker-js/faker": "^9.3.0",
    "dotenv": "^16.0.0"
  },
  "sideEffects": false
}
